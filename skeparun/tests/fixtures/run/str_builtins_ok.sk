import io;
import str;

fn main() -> Int {
  let s = "  SkEpA  ";
  let t = str.trim(s);
  let l = str.toLower(t);
  let u = str.toUpper(t);
  if (str.startsWith(l, "sk") && str.endsWith(u, "PA") && str.contains(l, "kep")) {
    io.printString(l);
    io.printString(u);
    return 0;
  }
  return 1;
}
